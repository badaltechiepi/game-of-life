node('master'){
    stage('git'){
        git 'https://github.com/badaltechiepi/game-of-life.git'
    }
    stage('build'){
         sh 'mvn clean package'
    }
    stage('test'){
        junit 'gameoflife-web/target/surefire-reports/*.xml'
    }
    stage('artifact'){
        archiveArtifacts artifacts: 'gameoflife-web/target/*.war'
    }
}